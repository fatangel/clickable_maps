<script type="text/javascript">
$(document).ready(function(){
/* Show jQuery is running */
$('h3').css({textDecoration: 'underline'});

$('#map').zoommap({
		// Width and Height of the Map
		width: '642px',
		height: '600px',

		//Misc Settings
		blankImage: '/images/maps/blank.gif',
		zoomDuration: 1000,
		bulletWidthOffset: '10px',
		bulletHeightOffset: '10px',

		//ids and classes
		zoomClass: 'zoomable',
		popupSelector: 'div.popup',
		popupCloseSelector: 'a.close',

		//Return to Parent Map Link
		showReturnLink: false,
		returnId: 'returnlink',
		returnText: 'Zur&uuml;ck zur &Uuml;bersichtskarte',

		//Initial Region to be shown
		map: {
			id: 'oesterreich',
			image: '/images/maps/<%=h @state.short_name %>.png',
			data: '/districts/<%=h @state.short_name %>',
            maps: [
			{

			}
			]
		}
	});
});
</script>

<h3><%=h @state.name %></h3>

<div id="map"></div>
